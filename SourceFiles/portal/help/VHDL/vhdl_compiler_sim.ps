} def                                                                       
/bus {					% x1 y1 x2 y2 -> -                  
	gsave bwidth setlinewidth newpath moveto lineto stroke grestore     
} def                                                                       
/label {                                % x y (text) -> -                   
        gsave                                                               
	3 1 roll                                                            
	moveto                                                              
        1 -1 scale                                                          
	show                                                                
        grestore                                                            
} def                                                                       
